{
    "name": "be-lenka/everifin-php-sdk",
    "description": "Everifin php sdk",
    "keywords": [
        "everifin",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://everifin.com",
    "license": "MIT",
    "authors": [
        {
            "name": "BeLenka Dev",
            "homepage": "https://www.belenka.com",
            "email": "dev+hello@belenka.dev"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2",
        "guzzlehttp/psr7": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": { "belenka\Everifin\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "belenka\Everifin\\Client\\Test\\" : "test/" }
    }
}
